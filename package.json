{
  "name": "vue-url-finder",
  "displayName": "Vue URL Finder",
  "description": "Finds URL routes where a Vue component is used.",
  "version": "0.1.2",
  "publisher": "rrgrs",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "vueUsageFinder.findUsage",
        "title": "Vue Usage: Find URL Usage"
      },
      {
        "command": "vueUsageFinder.clearCache",
        "title": "Vue Usage: Clear Cache"
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rrgrs/vue-url-finder-ext.git"
  },
  "scripts": {
    "vscode:prepublish": "npm run build",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "build": "node ./esbuild.config.js",
    "lint": "eslint src --ext ts"
  },
  "dependencies": {
    "@typescript-eslint/types": "^8.28.0",
    "@typescript-eslint/typescript-estree": "^8.28.0",
    "glob": "^10.0.0"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/node": "^20.0.0",
    "@types/vscode": "^1.80.0",
    "esbuild": "^0.25.1",
    "typescript": "^5.0.0"
  }
}
